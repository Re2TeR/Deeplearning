   "source": [
    "boxplot=plt.figure()\n",
    "axes1=boxplot.add_subplot(1,1,1)\n",
    "axes1.boxplot([tips[tips['sex']=='Female']['tip'],\n",
    "              tips[tips['sex']=='Male']['tip']],\n",
