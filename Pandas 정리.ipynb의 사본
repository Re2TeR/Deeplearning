      "[244 rows x 8 columns]\n"
     ]
    }
   ],
   "source": [
    "def recode_sex(sex):\n",
    "    if sex =='Female':\n",
    "        return 0\n",
    "    else:\n",
    "        return 1\n",
    "tips['sex_color']=tips['sex'].apply(recode_sex)\n",
    "print(tips)"
   ]
